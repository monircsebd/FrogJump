<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.monir.frogjump"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.test.monir.frogjump.HomePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.test.monir.frogjump.GamePage"
            android:label="@string/title_activity_game_page"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.test.monir.frogjump.AboutUs"
            android:label="@string/title_activity_about_us" >
        </activity>
    </application>

</manifest>